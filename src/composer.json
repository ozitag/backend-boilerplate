{
    "name": "ozitag/tager-backend",
    "type": "project",
    "description": "Tager Backend",
    "license": "MIT",
    "require": {
        "ozitag/tager-backend-core": "^1.0",
        "ozitag/tager-backend-sentry": "^1.0",
        "ozitag/tager-backend-admin": "^1.0",
        "ozitag/tager-backend-seo": "^1.0",
        "ozitag/tager-backend-mail": "^1.0",
        "ozitag/tager-backend-settings": "^1.0",
        "ozitag/tager-backend-menus": "^1.0",
        "ozitag/tager-backend-banners": "^1.0",
        "ozitag/tager-backend-pages": "^1.0",
        "ozerich/laravel-filestorage-db": "^0.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
